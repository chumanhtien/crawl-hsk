                        "tdtc": [
                            "Năm",
                            "Tuổi",
                            "Người đỗ cùng khoa gọi là đồng niên 同年. Hai nhà đi lại với nhau gọi là niên nghị 年誼",
                            "Ðược mùa"
                        ],
                        "tdpt": [
                            "năm",
